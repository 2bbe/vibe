{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "vibe",
    "version": "0.0.4"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "window": {
        "all": true
      },
      "app": {
        "all": false,
        "hide": true,
        "show": true
      },
      "os": {
        "all": true
      },
      "shell": {
        "all": true,
        "open": ".*"
      },
      "dialog": {
        "all": true,
        "ask": true,
        "confirm": true,
        "message": true,
        "open": true,
        "save": true
      },
      "fs": {
        "all": true,
        "scope": {
          "allow": [
            "**"
          ],
          "requireLiteralLeadingDot": false
        }
      },
      "protocol": {
        "asset": true,
        "assetScope": {
          "allow": [
            "**"
          ],
          "requireLiteralLeadingDot": false
        }
      },
      "path": {
        "all": true
      }
    },
    "bundle": {
      "active": true,
      "targets": [
        "nsis",
        "deb",
        "dmg"
      ],
      "identifier": "github.com.thewh1teagle.vibe",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "resources": [
        "locales/**/**"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "vibe",
        "width": 1440,
        "height": 900,
        "focus": true,
        "visible": true,
        "label": "main"
      }
    ]
  }
}